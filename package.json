{
	"name": "copy-code",
	"displayName": "copy-code",
	"description": "Copy code with automatic code-block formatter.",
	"version": "1.2.0",
	"publisher": "PeterBanfi",
	"author": {
		"name": "Peter Banfi"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/peterbanfi/copy_code"
	},
	"icon": "images/copy-code.png",
	"galleryBanner": {
		"color": "#00FFFC",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.51.0"
	},
	"homepage": "https://github.com/peterbanfi/copy_code/blob/master/README.md",
	"bugs": {
		"url": "https://github.com/peterbanfi/copy_code/issues",
		"email": "p6t6rbanfi@gmail.com"
	},
	"categories": [
		"Formatters",
		"Other"
	],
	"keywords": [
		"markdown",
		"clode-block",
		"copy"
	],
	"activationEvents": [
		"onCommand:copy-code.copycode"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "copy-code.copycode",
				"title": "Copy Code"
			}
		],
		"keybindings": [
			{
				"command": "copy-code.copycode",
				"key": "ctrl+Alt+C",
				"mac": "Cmd+Shift+F9"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint"
	},
	"devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"vscode-test": "^1.4.0"
	}
}