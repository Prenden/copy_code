{
	"name": "copy-code",
	"displayName": "copy-code",
	"description": "Copy code with template literals for easy paste to editors' whats supports markdown",
	"version": "1.0.0",
	"publisher": "PeterBanfi",
	"repository": {
		"url": "https://github.com/peterbanfi/copy_code"
	},
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:copy-code.copycode"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "copy-code.copycode",
				"title": "Copy Code"
			}
		],
		"keybindings": [
			{
				"command": "copy-code.copycode",
				"key": "ctrl+x+c",
				"mac": "cmd+x+c"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint"
	},
	"devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"vscode-test": "^1.4.0"
	}
}